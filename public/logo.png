<svg xmlns="http://www.w3.org/2000/svg" width="512" height="512" viewBox="0 0 512 512">
  <defs>
    <linearGradient id="logoGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#06b6d4;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#3b82f6;stop-opacity:1" />
    </linearGradient>
  </defs>

  <!-- Background Circle -->
  <circle cx="256" cy="256" r="240" fill="url(#logoGradient)"/>

  <!-- Letter S -->
  <text x="256" y="360" font-family="Arial, sans-serif" font-size="280" font-weight="bold" fill="white" text-anchor="middle">
    S
  </text>

  <!-- Code Brackets -->
  <text x="120" y="200" font-family="monospace" font-size="120" fill="white" opacity="0.3">
    &lt;
  </text>
  <text x="360" y="200" font-family="monospace" font-size="120" fill="white" opacity="0.3">
    /&gt;
  </text>
</svg>
